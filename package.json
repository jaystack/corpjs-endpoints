{
  "name": "corpjs-endpoints",
  "version": "0.0.1",
  "description": "Endpoint manager for microservices",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "build": "node_modules/.bin/tsc -p src --outDir lib",
    "build:w": "node_modules/.bin/tsc -p src --outDir lib -w",
    "build-tests": "node_modules/.bin/tsc -p test_src --outDir test",
    "build-tests:w": "node_modules/.bin/tsc -p test_src --outDir test",
    "pretest": "npm run build-tests",
    "test": "node_modules/.bin/mocha test/**/*.spec.js -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaystack/corpjs-endpoints.git"
  },
  "keywords": [
    "microservice",
    "endpoints",
    "corpjs"
  ],
  "author": "JayStack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaystack/corpjs-endpoints/issues"
  },
  "homepage": "https://github.com/jaystack/corpjs-endpoints#readme",
  "dependencies": {
    "corpjs-system": "~0.0.27",
    "fs-promise": "^2.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.39",
    "mocha": "^3.2.0",
    "typescript": "^2.2.0"
  }
}
